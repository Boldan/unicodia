  <group id="Prop">
   <group id="Head">
    <text id="NonChar">
     <orig>Non-character</orig>
     <au-cmt>Test: FFFF (go to FFFD, then manually)</au-cmt>
     <transl>非文字</transl>
    </text>
    <text id="Empty">
